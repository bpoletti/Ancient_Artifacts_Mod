		"sources": "https://github.com/bpoletti/Ancient_Artifacts_Mod"
	},
	"license": "MIT License",
	"icon": "assets/ancientartifacts/ancientartifactsicon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"net.eagle.ancientartifacts.AncientArtifacts"
		],
		"client": [
			"net.eagle.ancientartifacts.AncientArtifactsClient"
		]
	},
	"mixins": [
		"ancientartifacts.mixins.json"
	],
	"depends": {
		"fabricloader": ">=0.14.13",
		"minecraft": "~1.19.2",
		"java": ">=17",
		"fabric-api": "*"
